<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.jim.api.entities.blog.blogUserRoleEntity" table="user_role" schema="blog">
        <id name="id">
            <column name="id" sql-type="int(10) unsigned"/>
        </id>
        <property name="userId">
            <column name="user_id" sql-type="bigint(30) unsigned"/>
        </property>
        <property name="roleId">
            <column name="role_id" sql-type="int(10) unsigned"/>
        </property>
    </class>
</hibernate-mapping>